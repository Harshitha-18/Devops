pipeline {
    agent any 
    stages {
        stage('clean stage') { 
            steps {
                echo 'mvn clean'
            }
        }
        stage(' validate stage ') { 
            steps {
               echo 'validating the application'
            }
        }

        stage('compile stage') { 
            steps {
                echo 'compiling the application'
            }
        }
       
        stage('Testing stage') { 
            steps {
                echo 'Testing stage of the application'
            }
        }
        stage('Deploy stage') { 
            steps {
                echo 'Deploying the application'
            }
        }
}
}
