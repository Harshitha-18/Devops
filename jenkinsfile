pipeline {
    agent { dockerfile true }
    stages {
    stages {
        stage('Build') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('Test') {
            steps {
                sh 'node --version'
                sh 'svn --version'
            }
        }
        stage('deploy') { 
            steps {
                echo 'deploying the application'
            }
        }
    }
}
